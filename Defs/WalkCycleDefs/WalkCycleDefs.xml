<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!--
  passingBegin = 0f;
  up1          = 0.125f;
  contact1     = 0.25f;
  down1        = 0.375f;
  passingMid   = 0.5f;
  up2          = 0.625f;
  contact2     = 0.75f;
  down2        = 0.875f;
  passingEnd   = 1f;
  -->

  <FacialStuff.Defs.WalkCycleDef>

    <defName>Human_Walking</defName>
    <label>Human_Walking</label>

    <animation>

      <!--passingBegin-->
      <li>
        <keyFrameAt>0</keyFrameAt>
        <FootAngle>0</FootAngle>
        <FootPositionX>0</FootPositionX>
        <FootPositionY>0</FootPositionY>
        <HandsSwingAngle>0</HandsSwingAngle>
        <HandsSwingPosVertical>0</HandsSwingPosVertical>
        <BodyOffsetVertical>0</BodyOffsetVertical>
        <BodyAngle>0</BodyAngle>
        <BodyAngleVertical>0</BodyAngleVertical>
      </li>

      <!--up1 - right foot behind-->
      <li>
        <keyFrameAt>0.125</keyFrameAt>
        <FootPositionY>0.0125</FootPositionY>
        <BodyOffsetVertical>0.125</BodyOffsetVertical>
        <BodyAngle>-5</BodyAngle>
        <BodyAngleVertical>3</BodyAngleVertical>
      </li>

      <!--contact1-->
      <li>
        <keyFrameAt>0.25</keyFrameAt>
        <FootPositionX>-0.125</FootPositionX>
        <FootPositionY>0.025</FootPositionY>
        <HandsSwingAngle>-40</HandsSwingAngle>
      </li>

      <!--down1-->
      <li>
        <keyFrameAt>0.375</keyFrameAt>
        <FootAngle>45</FootAngle>
        <FootPositionY>0</FootPositionY>
        <HandsSwingAngle>-50</HandsSwingAngle>
        <HandsSwingPosVertical>-0.075</HandsSwingPosVertical>
        <BodyOffsetVertical>-0.03</BodyOffsetVertical>
        <BodyAngle>7</BodyAngle>
      </li>

      <!--passingMid-->
      <li>
        <keyFrameAt>0.5</keyFrameAt>
        <FootAngle>90</FootAngle>
        <FootPositionX>0</FootPositionX>
        <FootPositionY>0.125</FootPositionY>
        <HandsSwingAngle>0</HandsSwingAngle>
        <HandsSwingPosVertical>0</HandsSwingPosVertical>
        <BodyOffsetVertical>0</BodyOffsetVertical>
        <BodyAngle>0</BodyAngle>
        <BodyAngleVertical>0</BodyAngleVertical>
      </li>
      
      <!--up2 - right foot front-->
      <li>
        <keyFrameAt>0.625</keyFrameAt>
        <BodyOffsetVertical>0.125</BodyOffsetVertical>
        <BodyAngle>-5</BodyAngle>
        <BodyAngleVertical>-3</BodyAngleVertical>
      </li>

      <!--contact2-->
      <li>
        <keyFrameAt>0.75</keyFrameAt>
        <FootAngle>-45</FootAngle>
        <FootPositionX>0.125</FootPositionX>
        <FootPositionY>0.025</FootPositionY>
        <HandsSwingAngle>40</HandsSwingAngle>
        <BodyOffsetVertical>0</BodyOffsetVertical>
      </li>

      <!--down2-->
      <li>
        <keyFrameAt>0.875</keyFrameAt>
        <FootAngle>0</FootAngle>
        <FootPositionY>0</FootPositionY>
        <HandsSwingAngle>50</HandsSwingAngle>
        <HandsSwingPosVertical>0.075</HandsSwingPosVertical>
        <BodyOffsetVertical>-0.03</BodyOffsetVertical>
        <BodyAngle>7</BodyAngle>
      </li>

      <!--passingEnd-->
      <li>
        <keyFrameAt>1</keyFrameAt>
        <FootPositionX>0</FootPositionX>
        <FootPositionY>0</FootPositionY>
        <HandsSwingAngle>0</HandsSwingAngle>
        <HandsSwingPosVertical>0</HandsSwingPosVertical>
        <BodyOffsetVertical>0</BodyOffsetVertical>
        <BodyAngle>0</BodyAngle>
        <BodyAngleVertical>0</BodyAngleVertical>
      </li>

    </animation>

  </FacialStuff.Defs.WalkCycleDef>
 
  <FacialStuff.Defs.WalkCycleDef>

    <defName>Human_Jog</defName>
    <label>Human_Jog</label>

    <animation>
  
      <!--passingBegin-->
      <li>
        <keyFrameAt>0</keyFrameAt>
        <FootAngle>45</FootAngle>
        <FootPositionX>0</FootPositionX>
        <FootPositionY>0.1</FootPositionY>
        <HandsSwingAngle>-60</HandsSwingAngle>
        <HandsSwingPosVertical>0</HandsSwingPosVertical>
        <BodyOffsetVertical>0</BodyOffsetVertical>
        <BodyAngle>0</BodyAngle>
        <BodyAngleVertical>0</BodyAngleVertical>
      </li>

      <!--up1 - right foot front-->
      <li>
        <keyFrameAt>0.125</keyFrameAt>
        <FootAngle>10</FootAngle>
        <FootPositionX>0.08</FootPositionX>
        <FootPositionY>0.125</FootPositionY>
        <BodyOffsetVertical>0.1</BodyOffsetVertical>
        <BodyAngle>-5</BodyAngle>
        <BodyAngleVertical>-3</BodyAngleVertical>
      </li>

      <!--contact1-->
      <li>
        <keyFrameAt>0.25</keyFrameAt>
        <FootAngle>0</FootAngle>
        <FootPositionX>0.06</FootPositionX>
        <FootPositionY>0.025</FootPositionY>
        <HandsSwingAngle>40</HandsSwingAngle>
        <BodyOffsetVertical>0</BodyOffsetVertical>
      </li>

      <!--down1-->
      <li>
        <keyFrameAt>0.375</keyFrameAt>
        <FootAngle>0</FootAngle>
        <FootPositionY>0</FootPositionY>
        <HandsSwingAngle>50</HandsSwingAngle>
        <HandsSwingPosVertical>0.075</HandsSwingPosVertical>
        <BodyOffsetVertical>-0.02</BodyOffsetVertical>
        <BodyAngle>7</BodyAngle>
      </li>

      <!--passingMid-->
      <li>
        <keyFrameAt>0.5</keyFrameAt>
        <FootAngle>0</FootAngle>
        <FootPositionX>0</FootPositionX>
        <FootPositionY>0</FootPositionY>
        <HandsSwingAngle>0</HandsSwingAngle>
        <HandsSwingPosVertical>0</HandsSwingPosVertical>
        <BodyOffsetVertical>0</BodyOffsetVertical>
        <BodyAngle>0</BodyAngle>
        <BodyAngleVertical>0</BodyAngleVertical>
      </li>

      <!--up2 - right foot behind-->
      <li>
        <keyFrameAt>0.625</keyFrameAt>
        <FootAngle>12.5</FootAngle>
        <FootPositionX>-0.04</FootPositionX>
        <FootPositionY>0</FootPositionY>
        <BodyOffsetVertical>0.1</BodyOffsetVertical>
        <BodyAngle>-5</BodyAngle>
        <BodyAngleVertical>3</BodyAngleVertical>
      </li>

      <!--contact2-->
      <li>
        <keyFrameAt>0.75</keyFrameAt>
        <FootAngle>30</FootAngle>
        <FootPositionX>-0.1</FootPositionX>
        <FootPositionY>0.025</FootPositionY>
        <HandsSwingAngle>-40</HandsSwingAngle>
        <BodyOffsetVertical>0</BodyOffsetVertical>
      </li>
   
      <!--down2-->
      <li>
        <keyFrameAt>0.875</keyFrameAt>
        <FootAngle>45</FootAngle>
        <FootPositionY>0</FootPositionY>
        <HandsSwingAngle>-50</HandsSwingAngle>
        <HandsSwingPosVertical>-0.075</HandsSwingPosVertical>
        <BodyOffsetVertical>-0.02</BodyOffsetVertical>
        <BodyAngle>7</BodyAngle>
      </li>

      <!--passingEnd-->
      <li>
        <keyFrameAt>1</keyFrameAt>
        <FootAngle>50</FootAngle>
        <FootPositionX>0</FootPositionX>
        <FootPositionY>0.1</FootPositionY>
        <HandsSwingAngle>0</HandsSwingAngle>
        <HandsSwingPosVertical>0</HandsSwingPosVertical>
        <BodyOffsetVertical>0</BodyOffsetVertical>
        <BodyAngle>0</BodyAngle>
        <BodyAngleVertical>0</BodyAngleVertical>
      </li>

    </animation>

  </FacialStuff.Defs.WalkCycleDef>

  

</Defs>